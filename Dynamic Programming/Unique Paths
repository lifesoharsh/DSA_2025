class Solution {
public:
    
    int uniquePaths(int m, int n) {
        vector<vector<int>> paths(m+1, vector<int>(n+1, 0));
        paths[1][1] = 1;

        for(int i = 1; i <= m; i++){
            for(int j = 1; j <= n; j++){
                if(i == 1 && j == 1) continue;
                paths[i][j] = paths[i-1][j] + paths[i][j-1];
            }
        }

        return paths[m][n];
    }
};
