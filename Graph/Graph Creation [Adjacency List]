#include <iostream>
#include <unordered_map>
#include <list>
using namespace std;

class Graph{
public: 
   unordered_map<int,list<int>> adj;

   void addEdge(int u, int v, bool direction){
    adj[u].push_back(v);

    if(direction == 0){
        adj[v].push_back(u);
    }
   }

   void printAdjList(){
    for(auto i:adj){
        cout << i.first << "->";
        for(auto j:i.second){
            cout << j << ",";
        }
        cout << endl;
    }
   }
                               
};

int main(){
    int v;
    cout << "Enter the number of nodes ";
    cin >> v;

    int e;
    cout << "Enter the number of edges ";
    cin >> e;

    Graph g;

    for(int i = 0; i < e; i++){
        int u,v;
        cin >> u >> v;
        g.addEdge(u,v,false);
    }

    g.printAdjList();

    return 0;
}
